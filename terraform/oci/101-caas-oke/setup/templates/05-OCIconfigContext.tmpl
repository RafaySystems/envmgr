{{ $glbCtx := . }}
apiVersion: eaas.envmgmt.io/v1
Config context:
kind: ConfigContext
metadata:
  name: {{ $glbCtx.OCIConfigContext }}-{{ $glbCtx.projectName }}
  project: {{ $glbCtx.projectName }}
spec:
  envs:
  - key: TF_VAR_tenancy_ocid
    sensitive: true
    value: UPDATE_VALUE
  - key: OCI_CLI_TENANCY
    sensitive: true
    value: UPDATE_VALUE
  - key: TF_VAR_user_ocid
    sensitive: true
    value: UPDATE_VALUE
  - key: OCI_CLI_USER
    sensitive: true
    value: UPDATE_VALUE
  - key: TF_VAR_fingerprint
    sensitive: true
    value: UPDATE_VALUE
  - key: OCI_CLI_FINGERPRINT
    sensitive: true
    value: UPDATE_VALUE
  - key: TF_VAR_private_key_path
    value: oci-key
  - key: OCI_CLI_KEY_FILE
    value: /run/envmgmt.io/contextdata/oci-key
  - key: OCI_CLI_REGION
    value: us-phoenix-1
  files:
  - data: c2VhbGVkOi8vZmlsZXMuMA==
    name: oci-key
    sensitive: true
